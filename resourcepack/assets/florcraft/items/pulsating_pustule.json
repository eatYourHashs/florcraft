{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "cases": [
      {
        "when": "arid",
        "model": {
                "type": "minecraft:model",
                "model": "florcraft:item/pulsating_pustule_arid"
              }
      },
      {
        "when": "aqueous",
        "model": {
                "type": "minecraft:model",
                "model": "florcraft:item/pulsating_pustule_aqueous"
              }
      },
      {
        "when": "verdant",
        "model": {
                "type": "minecraft:model",
                "model": "florcraft:item/pulsating_pustule_verdant"
              }
      }
    ],
    "fallback": {
                "type": "minecraft:model",
                "model": "florcraft:item/pulsating_pustule_arid"
              }
  }
}